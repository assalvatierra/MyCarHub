@page
@model Webapp.Pages.LoginModel
@{
    
}

<h1>Login</h1>
<p>
    Server info: @Model.serverinfo
</p>
<form method="post">
    <div>
        <div>
            <label>Login</label>
            <input type="text" name="login" value="abel" />
        </div>
        <div>
            <label>Password</label>
            <input type="password" name="password" value="123456" />
        </div>
        <div>
            <input type="submit" value="login" />
        </div>
        <div>
            <p>@ViewData["status"]</p>
        </div>

    </div>
</form>



