@page
@model MyCarCare.Pages.Main.Garage.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>MyGarage</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MyGarage.Name" class="control-label"></label>
                <input asp-for="MyGarage.Name" class="form-control" />
                <span asp-validation-for="MyGarage.Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MyGarage.Location" class="control-label"></label>
                <input asp-for="MyGarage.Location" class="form-control" />
                <span asp-validation-for="MyGarage.Location" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MyGarage.Remarks" class="control-label"></label>
                <input asp-for="MyGarage.Remarks" class="form-control" />
                <span asp-validation-for="MyGarage.Remarks" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MyGarage.MyAccountId" class="control-label"></label>
                <select asp-for="MyGarage.MyAccountId" class ="form-control" asp-items="ViewBag.MyAccountId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
